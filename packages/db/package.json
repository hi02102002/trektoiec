{
	"name": "@trektoeic/db",
	"type": "module",
	"exports": {
		".": {
			"default": "./src/index.ts"
		},
		"./*": {
			"default": "./src/*.ts"
		},
		"./schema/*": {
			"default": "./src/schema/*.ts"
		},
		"./queries/toeic-max-tokens/*": {
			"default": "./src/queries/toeic-max-tokens/*.ts"
		},
		"./queries/toeic-max-tokens": {
			"default": "./src/queries/toeic-max-tokens/index.ts"
		}
	},
	"scripts": {
		"db:push": "drizzle-kit push",
		"db:studio": "drizzle-kit studio",
		"db:generate": "drizzle-kit generate",
		"db:migrate": "drizzle-kit migrate",
		"db:pull": "drizzle-kit pull",
		"check-types": "tsc --noEmit"
	},
	"devDependencies": {
		"drizzle-kit": "^0.31.2",
		"@types/pg": "^8.11.11",
		"@trektoeic/config": "workspace:*"
	},
	"peerDependencies": {
		"typescript": "^5"
	},
	"dependencies": {
		"drizzle-orm": "^0.44.2",
		"pg": "^8.14.1",
		"zod": "^4.1.11",
		"@trektoeic/env": "workspace:*",
		"@trektoeic/utils": "workspace:*"
	}
}
